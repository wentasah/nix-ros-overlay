{ lib, buildRosPackage, fetchurl, ament-cmake }:
buildRosPackage {
  pname = "ros-jazzy-shared-queues-vendor";
  version = "0.26.8-r1";

  src = fetchurl {
    url = "https://github.com/ros2-gbp/rosbag2-release/archive/release/jazzy/shared_queues_vendor/0.26.8-1.tar.gz";
    name = "0.26.8-1.tar.gz";
    sha256 = "15075d4dce4d3300b22849d17aee290c3bc5d4932bd349517327be3e26404c0d";
  };

  buildType = "ament_cmake";
